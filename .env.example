# 复制本文件为 .env 并填充实际值后再启动服务
RAGFLOW_API_BASE=http://localhost:9621
RAGFLOW_API_KEY=your-api-key

VANNA_API_BASE=http://localhost:8000
VANNA_API_KEY=your_vanna_api_key

########################################
# Vanna LLM Configuration
########################################

# 对话大模型名称（可选，默认 deepseek-chat）
VANNA_LLM_MODEL=deepseek-chat

# 对话大模型的 API Key（必填）
VANNA_LLM_API_KEY=your_llm_api_key

# 对话大模型的 Base URL（可选，默认 https://api.deepseek.com/v1）
VANNA_LLM_BASE_URL=https://api.deepseek.com/v1


########################################
# Vanna SQL / Postgres Configuration
########################################

# Postgres 连接串（必填，用于 RunSqlTool）
# 示例：postgresql://user:password@localhost:5432/dbname
VANNA_PG_CONN_STR=postgresql://user:password@localhost:5432/your_db


########################################
# Vanna Memory / ChromaDB Configuration
########################################

# Chroma collection 名称（可选，默认 vanna_memory）
VANNA_MEMORY_COLLECTION=vanna_memory

# Chroma 持久化目录（可选，默认 ./chroma_db）
VANNA_CHROMA_DIR=./chroma_db


########################################
# Vanna Embedding Service Configuration
########################################

# Embedding 接口 Base URL（必填或根据服务器要求）
# 示例（OpenAI 兼容接口）：https://api.openai.com/v1
VANNA_EMBED_BASE_URL=https://api.openai.com/v1

# Embedding 模型的 API Key（必填）
VANNA_EMBED_API_KEY=your_embedding_api_key

# Embedding 模型名称（可选，默认 qwen3-emb-0.6b）
VANNA_EMBED_MODEL=qwen3-emb-0.6b
